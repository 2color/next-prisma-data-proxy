generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id       Int       @id @default(autoincrement())
  title    String
  excerpt  String
  content  String?
  views    Int       @default(0)
  likes    Int       @default(0)
}